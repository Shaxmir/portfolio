script(type="text/x-template")#skills-list
    .vakues_skills
        ul.skill_list
            li.skill_item(v-for="skill in skills" :key="skill.skillsGroup")
                skills-row(
                    :skill="skill"
                )
script(type="text/x-template")#skills-row
    .front-end
        h2 {{skill.skillsGroup}}
        ul.rounds
            li.data(v-for="(skillPercent, skillName) in skill.skills") 
                skill(
                    :skillName="skillName",
                    :skillPercent="skillPercent"
                )
script(type="text/x-template")#skills
    .skill_round
        span.skill_name {{skillName}}
        svg(viewBox="0 0 100 100" preserveAspectRatio="none").round_svg
            circle.circle(cx="50" cy="50" r="40").circle_under
            circle.circle(cx="50" cy="50" r="40" ref="color-circle").circle_above
            